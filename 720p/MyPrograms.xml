<?xml version="1.0" encoding="UTF-8"?>
<!-- Veritas -->
<window id="1">
    <defaultcontrol always="true">50</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <views>50,511,512</views>
    <controls>
        <include>GlobalBackground</include>
        <control type="group">
            <include>Animation_WindowOpen</include>
            <include>Animation_LeftMenu</include>
            <include>View_50_List</include>
            <include>View_511_LauncherPosterTall</include>
            <include>View_512_LauncherPosterWide</include>
        </control>
        <control type="grouplist" id="9000">
            <posx>10</posx>
            <posy>140</posy>
            <width>220</width>
            <height>600</height>
            <onleft>50</onleft>
            <onright>50</onright>
            <onup>9000</onup>
            <ondown>9000</ondown>
            <orientation>vertical</orientation>
            <animation type="Conditional" condition="!ControlGroup(9000).HasFocus()">
                <effect type="fade" start="100" end="50" time="200" />
                <effect type="zoom" center="0,300" start="100" end="80" time="200" />
            </animation>
            <visible allowhiddenfocus="true">ControlGroup(9000).HasFocus</visible>
            <control type="button" id="2">
                <description>View</description>
                <width>220</width>
                <align>left</align>
            </control>
        </control>
    </controls>
</window>
